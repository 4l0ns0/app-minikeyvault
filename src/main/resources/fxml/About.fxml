<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bpnPrincipal" prefWidth="340.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.opencode.minikeyvault.view.AboutView">
   <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <GridPane style="-fx-background-color: white;" VBox.vgrow="ALWAYS">
              <columnConstraints>
                <ColumnConstraints hgrow="NEVER" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="313.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="NEVER" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="NEVER" />
                <RowConstraints vgrow="ALWAYS" />
                  <RowConstraints vgrow="NEVER" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="NEVER" />
              </rowConstraints>
               <children>
                  <Label fx:id="txtTitle" text="title" GridPane.columnSpan="2">
                     <font>
                        <Font size="11.0" />
                     </font></Label>
                  <Label fx:id="txtDetail" alignment="TOP_LEFT" contentDisplay="TOP" text="message detail" textAlignment="JUSTIFY" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="2">
                     <font>
                        <Font size="11.0" />
                     </font></Label>
                  <Label fx:id="txtDevBy" text="development by" GridPane.columnSpan="2" GridPane.rowIndex="4">
                     <font>
                        <Font size="11.0" />
                     </font></Label>
                  <Label fx:id="txtVersion" text="version" GridPane.columnSpan="2" GridPane.rowIndex="1">
                     <font>
                        <Font name="System Bold Italic" size="11.0" />
                     </font>
                  </Label>
                  <Hyperlink fx:id="lnkRepository" focusTraversable="false" text="url" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="3">
                     <font>
                        <Font size="11.0" />
                     </font></Hyperlink>
               </children>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" fillHeight="false" maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="btnClose" mnemonicParsing="false" text="Cerrar" HBox.hgrow="NEVER" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
